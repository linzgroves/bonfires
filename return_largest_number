function largestOfFour(arr) {
  // You can do this!
  var biggestNumber = 0;
  var newArray = [];

  for (var i in arr) {
    for (var j in arr[i]) {
      if (arr[i][j] > biggestNumber) {
        biggestNumber = arr[i][j];
        
      }
    }
     newArray.push(biggestNumber); 
  }
    

 return newArray;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
